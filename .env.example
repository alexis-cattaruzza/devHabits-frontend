# DevHabits Frontend - Environment Variables Template
# Copy this file to .env for local development
# DO NOT commit .env file to version control

# API Configuration
VITE_API_URL=http://localhost:8080/api
VITE_BACKEND_URL=http://localhost:8080
VITE_FRONTEND_URL=http://localhost:4200

# GitHub OAuth Configuration
VITE_GITHUB_CLIENT_ID=your_github_client_id_here
VITE_GITHUB_REDIRECT_URI=http://localhost:8080/login/oauth2/code/github

# Storage Keys (usually don't need to change these)
VITE_TOKEN_KEY=devhabits_access_token
VITE_REFRESH_TOKEN_KEY=devhabits_refresh_token
